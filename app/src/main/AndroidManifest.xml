<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.codersoftech.Trademo">

    <application
        android:allowBackup="true"
        android:icon="@drawable/rupee"
        android:label="@string/app_name"
        android:roundIcon="@drawable/rupee"
        android:supportsRtl="true"
        android:theme="@style/Theme.MoneyBox">
        <activity android:name="com.codersoftech.Trademo.LoginActivity"></activity>
        <activity android:name="com.codersoftech.Trademo.FinalTrade" />
        <activity android:name="com.codersoftech.Trademo.QviewActivity" />
        <activity android:name="com.codersoftech.Trademo.MarketActivity" />
        <activity android:name="com.codersoftech.Trademo.MainActivity" />
        <activity android:name="com.codersoftech.Trademo.SignUpActivity" />
        <activity android:name="com.codersoftech.Trademo.SplashActivity">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
    </application>

</manifest>